generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("DATABASE_URL")
}

model Image {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  name      String   @db.VarChar(255)
  url       String?
}
